	"editorialNote": {
		"en": "The cmi5 profile is a specification that expands xAPI into the LMS-centric, single-learner distributed learning model.  It can be used in other ways, but the primary use case is to behave in a traditional LMS model."
	},
	"wasGeneratedBy": {
		"name": {
			"en": "Advanced Distributed Learning Initiative"
		}
	},
	"activity-types": [{
		"@id": "https://w3id.org/xapi/cmi5/activities/block",
		"@type": "ActivityType",
		"definition": {
			"en": "A block represents a number of Assignable Units of which progress (completion/success) is rolled up to.  In cmi5 it is every level above the AU and below the Course."
		},
		"prefLabel": {
			"en": "block"
		},
		"scopeNote": {
			"en": "Metadata consists of id, title, description, and objectives."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/activities/course",
		"@type": "ActivityType",
		"definition": {
			"en": "A course represents an amount of content that is published and registered for with the purpose of gaining completion.  It is represented with a Course Structure Format in cmi5 as the highest level of content (above Block and AU)."
		},
		"prefLabel": {
			"en": "course"
		},
		"scopeNote": {
			"en": "Metadata consists of id, title, description."
		},
		"closelyRelatedNaturalLanguageTerm": {
			"@id": "http://wordnet-rdf.princeton.edu/wn31/100886144-n"
		}
	}],
	"extensions": [{
		"@id": "https://w3id.org/xapi/cmi5/context/extensions/sessionid",
		"@type": "Extension",
		"definition": {
			"en": "A unique String identifier for a single AU launch session based on actor and course registration. Generated by LRS and must be in all cmi5 defined and cmi5 allowed Statements.  Used as a context extension."
		},
		"prefLabel": {
			"en": "sessionid"
		},
		"scopeNote": {
			"en": "The value for session ID is generated by the LMS. The LMS MUST also record the session ID in the State API prior to launching an AU. The LMS MUST also provide the session ID in the context as an extension for all cmi5 defined and cmi5 allowed statements it makes directly in the LRS.  An AU MUST include the session ID provided by the LMS in the context as an extension for all cmi5 defined and cmi5 allowed statements it makes directly in the LRS."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/context/extensions/masteryscore",
		"@type": "Extension",
		"definition": {
			"en": "Decimal value between 0 and 1 indicating the score needed to provide a passed Statement.  Determined on an AU plus registration basis. Used as a context extension."
		},

		"prefLabel": {
			"en": "masteryscore"
		},
		"scopeNote": {
			"en": "LMS MUST add masteryScore to the context of a Launched statement when it is provided in the LMS launch data.  An AU MUST include the masteryScore value provided by the LMS in the context as an extension for passed/failed Statements it makes based on the masteryScore."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/context/extensions/launchurl",
		"@type": "Extension",
		"definition": {
			"en": "Fully qualified URL equivalent to the one the LMS used to launch the AU and without name/value pairs.  Used as a context extension."
		},

		"prefLabel": {
			"en": "launchurl"
		},
		"scopeNote": {
			"en": "The LMS MUST put a fully qualified URL equivalent to the one that the LMS used to launch the AU without the name/value pairs included as defined in the context extensions of the Launched statement."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/result/extensions/session",
		"@type": "Extension",
		"definition": {
			"en": "Used in contexttemplate.  Value is used to populate/update session_id."
		},
		"prefLabel": {
			"en": "session"
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/context/extensions/launchParameters",
		"@type": "Extension",
		"definition": {
			"en": "The launch parameters used to launch this AU.  Parameters are endpoint, fetch, actor, registration, and activityId.  Used as a context extension."
		},
		"prefLabel": {
			"en": "launchParameters"
		},
		"scopeNote": {
			"en": "LMS MUST add launchParameters to the context of a Launched statement when it is provided in the LMS launch data."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/context/extensions/moveon",
		"@type": "Extension",
		"definition": {
			"en": "Controlled vocabulary which tells the conditions under which the AU is considered satisfied for the purposes of determining overall completion or prerequisites for other activities.  Used as a context extension.  "
		},
		"prefLabel": {
			"en": "moveon"
		},
		"scopeNote": {
			"en": "LMS MUST add moveOn to the context of a Launched statement."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/context/extensions/publisherid",
		"@type": "Extension",
		"definition": {
			"en": "A unique IRI for a single AU that is used to identify it as belonging to the publisher. Provided in Course Structure and must be in all cmi5 defined and cmi5 allowed Statements. Used as a context extension."
		},
		"prefLabel": {
			"en": "publisherid"
		},
		"scopeNote": {
			"en": "The LMS MUST record the publisher ID in the State API prior to launching an AU. The LMS MUST also provide the publisher ID in the context as an extension for all cmi5 defined and cmi5 allowed statements it makes directly in the LRS.  An AU MUST include the publisher ID provided by the LMS in the context as an extension for all cmi5 defined and cmi5 allowed statements it makes directly in the LRS."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/result/extensions/reason",
		"@type": "Extension",
		"definition": {
			"en": "Indicates why the Activity/AU was waived. Used as a result extension."
		},
		"prefLabel": {
			"en": "reason"
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/context/extensions/launchmode",
		"@type": "Extension",
		"definition": {
			"en": "String indicating which mode the AU was launched with by the LMS.  Used as a context extension."
		},
		"prefLabel": {
			"en": "launchmode"
		},
		"scopeNote": {
			"en": "LMS MUST add launchMode to the context of a Launched statement."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/result/extensions/progress",
		"@type": "Extension",
		"definition": {
			"en": "An integer value bewteen 0 and 100 (inclusive) indicating the completion of the Activity/AU as a percentage.  Used as a result extension."
		},
		"prefLabel": {
			"en": "progress"
		}
	}],
	"references": [{
		"@id": "http://adlnet.gov/expapi/activities/objective",
		"scopeNote": {
			"en": "This activity is only defined in the Course Structure Format (CSF). Objectives usage outside of course structure is not defined. The same Objective can be referenced by multiple AU or Blocks. There are 2 ways to interpret meeting an Objective. One or All elements referencing the objective need to be satisfied (or met moveOn criteria)."
		}
	}, 	
		"@id": "http://adlnet.gov/expapi/verbs/completed",
		"scopeNote": {
			"en": "The AU MUST record a statement containing the Completed verb when the learner has experienced all relevant material in an AU. The AU MUST NOT issue multiple statements with Completed for the same AU within a given AU session or course registration for a given learner."
		}
	}, 
	{
		"@id": "http://adlnet.gov/expapi/verbs/initialized",
		"scopeNote": {
			"en": "An Initialized statement is used by the AU to indicate that it has been fully initialized and MUST follow the Launched statement created by the LMS within a reasonable period of time.The AU MUST use Initialized in the first statement (of any kind) in the AU session. The AU MUST NOT issue multiple statements with Initialized for the same AU within a given AU session."
		}
	}, 
	{
		"@id": "http://adlnet.gov/expapi/verbs/terminated",
		"scopeNote": {
			"en": "The LMS MUST use the Terminated statement to determine that the AU session has ended. Upon receiving a Terminated statement, the LMS MUST wait a specified period of time (defined by the LMS implementation) after which it MUST reject statements (of any kind) for the AU session."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/adl/verbs/abandoned",
		"scopeNote": {
			"en": "In the absence of a Terminated statement, the LMS MUST make the determination if an AU abnormally terminated a session by monitoring new statement or state API requests made for the same learner/course registration for a different AU. The LMS MUST record an Abandoned statement on behalf of the AU indicating an abnormal session termination. After recording an Abandoned statement, the LMS MUST NOT allow any additional statements to be recorded for that session."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/adl/verbs/waived",
		"scopeNote": {
			"en": "The LMS MUST use this verb in a statement recorded in the LRS when it determines that the AU may be waived. A statement containing a Waived verb MUST include a reason in the extension property of the Statement Result."
		}
	}, 
	{
		"@id": "http://adlnet.gov/expapi/verbs/failed",
		"scopeNote": {
			"en": "The AU MUST record a statement containing the Failed verb when the learner has attempted and failed the AU. If the Failed statement contains a (scaled) score, the (scaled) score MUST be less than the masteryScore indicated in the LMS Launch Data. (See xAPI State Data Model, Section 10.0 - masteryScore)."
		}
	}, 
	{
		"@id": "http://adlnet.gov/expapi/verbs/launched",
		"scopeNote": {
			"en": "The LMS MUST use this verb in a statement recorded in the LRS before launching an AU. The LMS MUST NOT issue multiple statements with Launched for the same AU within a given AU session.  A Launched statement is used to indicate that the LMS has launched the AU. It SHOULD be used in combination with the Initialized statement sent by the AU in a reasonable period of time to determine whether the AU was successfully launched."
		}
	}, 
	{
		"@id": "http://adlnet.gov/expapi/verbs/passed",
		"scopeNote": {
			"en": "The AU MUST record a statement containing the Passed verb when the learner has attempted and passed the AU. If the Passed statement contains a (scaled) score, the (scaled) score MUST be equal to or greater than the masteryScore indicated in the LMS.  The LMS MUST use either Passed or Completed statements (or both) based on the moveOn criteria for the AU as provided in the LMS Launch Data. Launch Data."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/adl/verbs/satisfied",
		"scopeNote": {
			"en": "The LMS MUST use the Satisfied statement when the learner has met the moveOn criteria of all AU's in a block. In this statement the LMS MUST use the LMS generated block id as the Object id and use  =https://w3id.org/xapi/cmi5/activitytype/block as the value of the type property in the Object's Definition."
		}
	}, 
	{
		"@id": "http://adlnet.gov/expapi/verbs/voided",
		"scopeNote": {
			"en": "No cmi5-defined Statements can use Voided."
		}
	}],
	"categories": [{
		"@id": "https://w3id.org/xapi/cmi5/context/categories/moveon",
		"@type": "@id",
		"definition": {
			"en": "Indicates a Statement with a success or completion result property. Intended to be referenced by a context category."
		},
		"rdfs:isDefinedBy": {
			"@id": "https://w3id.org/xapi/cmi5"
		},
		"prefLabel": {
			"en": "moveon"
		},
		"scopeNote": {
			"en": "cmi5 defined statements with a Result object that include either success or completion properties MUST have an Activity object with an id of https://w3id.org/xapi/cmi5/context/categories/moveon in the category context activities list. Other statements MUST NOT include this Activity."
		}
	}, 
	{
		"@id": "https://w3id.org/xapi/cmi5/context/categories/cmi5",
		"@type": "@id",
		"definition": {
			"en": "Indicates a cmi5 defined Statement. Intended to be referenced by a context category."
		},
		"rdfs:isDefinedBy": {
			"@id": "https://w3id.org/xapi/cmi5"
		},
		"prefLabel": {
			"en": "cmi5"
		},
		"scopeNote": {
			"en": "An Activity object with an id of https://w3id.org/xapi/cmi5/context/categories/cmi5 in the category context activities list to be used in cmi5 defined Statements."
		}
	}]
}
{
    "@context": {
        "dcterms": "http://purl.org/dc/terms/",
        "foaf": "foaf: http://xmlns.com/foaf/0.1/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "prov": "http://www.w3.org/ns/prov#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",        
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "xapi": "https://w3id.org/xapi/ontology#",        
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "versions": {
            "@reverse": "prov:specializationOf",
            "@type": "skos:ConceptScheme"
        },
        "created": {
          "@id": "dcterms:created",
          "@type": "xsd:date"
        },
        "modified": {
          "@id": "dcterms:modified",
          "@type": "xsd:date"
        },  
        "ConceptScheme": "skos:ConceptScheme",
        "Verb": "xapi:Verb",
        "ActivityType": "xapi:ActivityType",
        "Extension": "xapi:Extension",
        "broadMatch": "skos:broadMatch",
        "narrowMatch": "skos:narrowMatch",
        "broader": "skos:broader",
        "narrower": "skos:narrower",
        "closelyRelatedNaturalLanguageTerm": "xapi:closelyRelatedNaturalLanguageTerm",                  
        "related": "skos:related",
        "relatedMatch": "skos:relatedMatch",
        "exactMatch": "skos:exactMatch",
        "closeMatch": "skos:closeMatch",      
        "Restriction": "owl:Restriction",
        "Datatype": "rdfs:Datatype",
        "DatatypeProperty": "owl:DatatypeProperty",
        "value": "prov:value",
        "onDatatype": "owl:onDatatype",
        "onProperty": "owl:onProperty",
        "withRestrictions": "owl:withRestrictions",
        "minInclusive": "xsd:minInclusive",
        "maxInclusive": "xsd:maxInclusive",
        "wasRevisionOf": {
            "@id": "prov:wasRevisionOf",
            "@type": "@id"
        },
        "wasGeneratedBy": {
            "@id": "prov:wasGeneratedBy",
            "@type": "prov:Activity"
        },
        "name": {
            "@id": "http://xmlns.com/foaf/0.1/name",
            "@container": "@language"
        },
        "vocabulary": {
            "@id": "skos:inScheme",
            "@type": "@id"
        },
        "prefLabel": {
            "@id": "skos:prefLabel",
            "@container": "@language"
        },
        "altLabel": {
            "@id": "skos:altLabel",
            "@container": "@language"
        },
        "thirdPartyLabel": {
            "@id": "xapi:thirdPartyLabel",
            "@container": "@language"
        },
        "definition": {
            "@id": "skos:definition",
            "@container": "@language"
        },
        "scopeNote": {
            "@id": "skos:scopeNote",
            "@container": "@language"
        },  
        "editorialNote": {
            "@id": "skos:editorialNote",
            "@container": "@language"
        },               
        "examples": {
            "@id": "skos:example",
            "@type": "xapi:Statement"
        },  
        "verbs": {
            "@reverse": "skos:inScheme",
            "@type": "xapi:Verb"
        },
        "activity-types": {
            "@reverse": "skos:inScheme",
            "@type": "xapi:ActivityType"
        },     
        "extensions": {
            "@reverse": "skos:inScheme",
            "@type": "xapi:Extension"
        }, 
        "datatype-properties": {
            "@reverse": "skos:inScheme",
            "@type": "owl:DatatypeProperty"
        },                     
        "references": {
            "@reverse": "xapi:referencedBy",
            "@type": "skos:Concept"
        }
    }
}
